set nocompatible #use vim's default features
set autoindent 
set cindent
#set smartindent
set tabstop=4
set shiftwidth=4
set textwidth=80
set nowrapscan
#set nojinspaces
set nobackup
set ruler
set nu
set showcmd
set showmatch
set ignorecase
set incsearch
set title
set en=UTF-8
set fileencodings=UTF-8
syntax on

